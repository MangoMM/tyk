{
  "name": "Tyk Serverless API",
  "api_id": "serverless",
  "org_id": "default",
  "definition": {
    "location": "header",
    "key": "version"
  },
  "version_data": {
    "not_versioned": true,
    "default_version": "Default",
    "versions": {
      "Default": {
        "name": "Default",
        "expires": "3000-01-02 15:04",
        "use_extended_paths": true,
        "extended_paths": {
          "serverless": [
            {
              "path": "serverless",
              "method": "POST",
              "provider": "aws-lambda",
              "provider_config": {
                "Region": "eu-west-2",
                "Name": "HelloWorldGoFunction",
                "Version": "$LATEST"
              }
            }
          ],
          "ignored": [],
          "white_list": [],
          "black_list": []
        }
      }
    }
  },
  "proxy": {
    "listen_path": "/",
    "target_url": "http://httpbin.org"
  },
  "domain": "serverless.com",
  "use_keyless": true,
  "auth": {
    "auth_header_name": ""
  }
}
